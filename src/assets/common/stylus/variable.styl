//文字体一行超出省略
textOverFlow(){
    white-space: nowrap;
    text-overflow: ellipsis;
    overflow: hidden;
}
textOverFlow2(){
    display: -webkit-box;
    -webkit-line-clamp: 2;
    overflow: hidden;
    text-overflow: ellipsis;
    -webkit-box-orient: vertical;
}
